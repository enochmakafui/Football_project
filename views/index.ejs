<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Football App ⚽</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="main.css">
  </head>
  <body>
   
    <nav class="navbar navbar-expand-lg  bg-dark  "data-bs-theme="dark">
      <div class="container-fluid">
        <a class="navbar-brand mx-auto " href="#">Football Highlights ⚽</a>
       
        
      </div>
    </nav>
    <% if(locals.data){ %>
      <div class="container-fluid">
      <% for(var i =0;i<data.response.length;i++){ %>
        
            <div class="col-lg-3 col-md-4 col-sm-6  bg-dark imagebox ">
              <a href="<%= data.response[i]. matchviewUrl %>" >
              <p><%= data.response[i].title %></p>
              <img src="<%= data.response[i].thumbnail %>" alt="" srcset="">
            </a>
            </div>
         
         
    
        <% } %>
      </div>
     
      <% } %>
    <footer>
      <p>Copyright ©<%=  new Date().getFullYear();%>
      </p>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>